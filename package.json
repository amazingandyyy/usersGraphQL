{
  "name": "users",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "babel-watch server.js",
    "precommit": "lint-staged",
    "test": "lint-staged && eslint ."
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.15.3",
    "express-graphql": "^0.6.5",
    "graphql": "^0.9.6",
    "lodash": "^4.17.4",
    "prettier": "^1.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-watch": "^2.0.6",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.1"
  },
  "babel": {
    "presets": [
      "babel-preset-env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "plugins": [
      "react"
    ],
    "parser": "babel-eslint",
    "rules": {
      "react/react-in-jsx-scope": 0
    },
    "globals": {
      "React": true
    },
    "env": {
      "node": true,
      "es6": true
    }
  }
}
